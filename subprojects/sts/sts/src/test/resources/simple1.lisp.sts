(system (var r Bool)
        (var x Int)
  (init (not r))
  (init (= x 1))
  (trans (>= x 1))
  (trans (<= x 4))
  (trans (prime (>= x 1)))
  (trans (prime (<= x 4)))
  (trans (= (prime x) (ite (and (< x 4) (not r)) (+ x 1) 1)))
  (trans (or (and (prime r) (= x 2)) (not (prime r))))
  (prop (=> r (/= x 2))))
